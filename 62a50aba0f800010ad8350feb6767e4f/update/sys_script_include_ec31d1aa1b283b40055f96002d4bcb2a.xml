        <api_name>sn_kpi_composer.projectIndicatorDefinitions</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description>Project indicator definitions related functions</description>
        <name>projectIndicatorDefinitions</name>
        <script><![CDATA[ function projectIndicatorDefinitions(input) { 
	 
	if (!input || !input.project || !input.project.sys_id) {
		input = {};
		input.project = {};
		input.project.sys_id = "NO_PROJECT_SYS_ID";
	} 

	var entityApi = new sn_kpi_composer.EntityApi();	 
	var data = entityApi.getData(input);	
	var indicators = [];
	for(var idx = 0; idx<data.indicatordefinitions.length; idx++) {
		indicators.push(data.indicatordefinitions[idx].sys_id);		
	}
	return indicators;
}]]></script>
