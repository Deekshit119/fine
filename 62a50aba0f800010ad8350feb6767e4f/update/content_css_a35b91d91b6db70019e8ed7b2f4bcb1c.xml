    border-bottom: 3px solid #1f8476;&#13;
    color: #1f8476;&#13;
}&#13;
.pa-overview-container .pa-overview-tabs-and-filters .pa-overview-filters-container {&#13;
    float: right;&#13;
    padding: 10px 0px 10px 0px;&#13;
}&#13;
.rtl .pa-overview-container .pa-overview-tabs-and-filters .pa-overview-filters-container {&#13;
    float: left;&#13;
}&#13;
.pa-overview-container .pa-overview-tabs-and-filters .pa-overview-search-box-container {&#13;
    display: inline-block;&#13;
