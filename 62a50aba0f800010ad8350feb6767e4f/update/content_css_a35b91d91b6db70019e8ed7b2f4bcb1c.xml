    outline: none;&#13;
}&#13;
.pa-overview-container .pa-overview-header .title {&#13;
    display: inline-block;&#13;
    font-size: 15px;&#13;
    font-weight: bold;&#13;
    padding: 6px 8px 7px 5px;&#13;
