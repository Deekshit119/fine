}&#13;
.pa-overview-card .pa-overview-info-date {&#13;
    color: #848b92;&#13;
    font-size: 12px;&#13;
    width: 100%;&#13;
    overflow: hidden;&#13;
    white-space: nowrap;&#13;
    text-overflow: ellipsis;&#13;
}&#13;
.pa-overview-card-element .buttoncontainer {&#13;
    position: absolute;&#13;
    top: 20px;&#13;
    right: 20px;&#13;
}&#13;
.rtl .pa-overview-card-element .buttoncontainer {&#13;
