}&#13;
.sn-btn-outline-secondary {&#13;
    border: 1px solid grey;&#13;
  }&#13;
.sn-btn-negative {&#13;
background-color: #FC4E4B;&#13;
border-color: #FC4E4B;&#13;
color: white;&#13;
}&#13;
.sn-btn-negative:hover, .sn-btn-negative:focus {&#13;
background-color: #ff6764;&#13;
color: white;&#13;
